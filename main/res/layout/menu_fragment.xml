        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Draugai" />

    <Button
        android:id="@+id/group_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GrupÄ—" />

    <Button
        android:id="@+id/pref_btn"
        android:layout_width="match_parent"
